<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PrimaryController">
   <children>
      <Button fx:id="primaryButton1" onAction="#switchToDrink" prefHeight="80.0" prefWidth="150.0" text="Order Drink" translateX="175.0" translateY="-20.0">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Label text="Select one to Proceed" translateY="-95.0">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Label>
      <Button fx:id="primaryButton" onAction="#switchToSecondary" prefHeight="80.0" prefWidth="150.0" text="Order Pizza" translateX="-175.0" translateY="-160.0">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Label prefHeight="18.0" prefWidth="160.0" text="Select to order either a preset" translateX="-175.0" translateY="-170.0">
         <font>
            <Font name="Franklin Gothic Demi" size="12.0" />
         </font></Label>
      <Label text="or custom pizza" translateX="-175.0" translateY="-190.0">
         <font>
            <Font name="Franklin Gothic Demi" size="12.0" />
         </font></Label>
      <Label text="Select to Order Water" translateX="175.0" translateY="-245.0">
         <font>
            <Font name="Franklin Gothic Demi" size="12.0" />
         </font></Label>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
